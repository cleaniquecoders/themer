{
    "name": "cleaniquecoders/themer",
    "description": "Laravel Theme",
    "keywords": ["package", "laravel", "theme", "make"],
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\Themer\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\Themer\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/filesystem" : "^5.6",
        "illuminate/http" : "^5.6",
        "illuminate/support": "^5.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench": "3.6.*"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "CleaniqueCoders\\Themer\\ThemerServiceProvider"
            ],
            "aliases": {
                "Themer": "CleaniqueCoders\\Themer\\ThemerFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}